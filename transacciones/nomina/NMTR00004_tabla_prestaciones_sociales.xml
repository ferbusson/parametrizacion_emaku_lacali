                        (Clasificacion==19||Clasificacion==20)?
                            dias:
                        (LN_LicenciaNoRemunerada!=0||SC_SuspensionContrato!=0)?
                          dias-(LN_LicenciaNoRemunerada+SC_SuspensionContrato):                        
                        (IN_Ingreso!=0)?
