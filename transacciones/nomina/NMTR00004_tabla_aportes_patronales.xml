                    <arg attribute="beanshell">Dias=
                    <!--Sep 23 2025: Quito la condicion anterior en la que evaluaba directamente si habia ingreso y retiro -->
                      "9".equals(Clasificacion)?
                          RE-IN>0?
                            <!-- RE-IN-LN-SC: -->
                            (RE-IN): <!--Sep 23 2025: Pongo el +1 Oct 20 lo quito para que el calculo de los dias sea correcto -->
                          IN!=0?
                            dias-(IN-1):
                          dias:
                        "10".equals(Clasificacion)?
                            VC!=0 &amp;&amp; RE!=0? <!--VC y RE != 0-->
                            RE-(VC+IG+IL+LM+LP+LR+LN+SC):
                          VC!=0? <!--VC !=0-->
                            dias-(VC+IG+IL+LM+LP+LR+LN+SC):
                          RE-IN>0? <!--RE - IN > 0 -->
                            ((RE-IN))-(IG+IL+LM+LP+LR+LN+SC):<!--Sep 23 2025: Pongo el +1 Oct 20 lo quito para que el calculo de los dias sea correcto -->
                          IN!=0? <!--IN != 0-->
                            dias-(IG+IL+LM+LP+LR+LN)-(IN-1):                          
                          dias-(IG+IL+LM+LP+LR+LN+SC):
                        "11".equals(Clasificacion)||"12".equals(Clasificacion)||"13".equals(Clasificacion)? <!-- Agosto 31 se agregan 12 y 13 (salud y pension) en revision caso salario superior a 25 ssmm -->
                          RE-IN>0? <!--RE - IN > 0 -->
                            ((RE-IN))-(IG+IL+LM+LP+LN+SC):<!--Sep 23 2025: Pongo el +1 Oct 20 lo quito para que el calculo de los dias sea correcto -->
                          IN!=0? <!--IN != 0-->
                            dias-(IG+IL+LM+LP+LN)-(IN-1):
                          dias-(IG+IL+LM+LP+LN+SC):
                        RE-IN>0? <!--RE - IN > 0 -->
                          ((RE-IN)+1)-(IG+IL+LM+LP+LR+LN+SC+VC+VP):<!--Sep 23 2025: Pongo el +1 para que el calculo de los dias sea correcto -->
                        IN!=0? <!--IN != 0-->
                          (dias-(IN-1))-(IG+IL+LM+LP+LR+LN+SC+VC+VP):
                        dias-(IN+IL+LM+LP+LR+LN+SC+VC)</arg>
                    
                    <!-- <arg attribute="formula">aq=m+n+o+p+ah+ap</arg> --> <!-- Marzo 2 de 2023 se cambia para cumplir condiciones reglamentadas junto con la adicion de las dos ultimas cols as y at -->
                    <arg attribute="beanshell">salarioSumado=eordinario+edominicales+recargos+dominical+otrosdevxvalor+devporcentual+devengadosalariobase</arg> <!-- Cambia de formula a beanshell Ago 30 de 2023 -->

                    <arg attribute="beanshell">ValorSB2=((ValorB/30.0)*Dias)</arg> <!-- Cambia de formula a beanshell Ago 30 de 2023 -->

                    <arg attribute="beanshell">ValorSB=
                                            salarioBasico&lt;salariominimo&amp;&amp;salarioSumado&gt;salariominimo&amp;&amp;("9".equals(Clasificacion))?
                                                (((((salarioBasico/dias)*(dias-(IG+IL+LM+LP))+otrosdevxvalor)+((salariominimo/30.0)*(IG+IL+LM+LP)))/30.0)*Dias): <!-- se compone una nueva base de liquidación, a partir de requerimiento Johanan 4 enero 2022-->
                                            salarioSumado&gt;(salariominimo*25)&amp;&amp;("10".equals(Clasificacion))?
                                                (((salariominimo*25)-salarioBasico)+devengadosalariobase): <!-- Si el salario + comisiones es mayor a 25 sal minimos la base es: 25 salarios min - salarioBasico + devengadosSalarioBase - Ago 30 2023 -->
                                            salarioSumado&gt;salariominimo*25&amp;&amp;(("8".equals(Clasificacion))||("9".equals(Clasificacion)))?((salariominimo*25)/30.0)*Dias: <!-- Si el salario + comisiones > 25 sal minimos la base son los 25 salarios min Ago 30 2023 -->
                                            salarioBasico&lt;salariominimo&amp;&amp;salarioSumado&gt;salariominimo&amp;&amp;("10".equals(Clasificacion))?
                                                (salarioBasico+otrosdevxvalor)-otrosdevprovisionado1:
                                                
                                                 <!--((salarioSumado/30)*Dias): Valor antes del cambio -->
                                            salarioBasico&lt;salariominimo&amp;&amp;salarioSumado&lt;salariominimo&amp;&amp;("9".equals(Clasificacion)||"10".equals(Clasificacion))?
                                            
                                                ((salariominimo/30.0)*Dias):
                                            eordinario+edominicales+recargos+dominical+ValorSB2+otrosdevporcentual+otrosdevxvalor</arg>    
                    
                    <arg attribute="beanshell">B_Liquidacion=(("11".equals(Clasificacion))||("12".equals(Clasificacion))||("13".equals(Clasificacion)))?(ValorSB+Vacaciones+vacacionesacum):ValorSB</arg> 

                    <!-- Mayo 3 2022 en conversación con Johana las vacaciones se deben sumar al salario en el caso de Caja Ccompensacion Fami, la anterior condición solo teńia en cuenta este aspecto cuando habia retiro por eso la corrijo Fer. -->

                    <!-- Junio 01 2022 se adicionaron ICBF(13) y Sena(12) a la formula anterior -->
                    
                    <arg attribute="beanshell">Valor=ROUND((B_Liquidacion*Porcentaje/100.0),100)</arg> <!-- Cambia de formula a beanshell Ago 30 de 2023 -->

                    
                    

